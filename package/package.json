  "exports": {
    "./components": {
      "import": "./dist/components.es.js",
      "require": "./dist/components.cjs.js",
      "types": "./dist/components/index.d.ts"
    },
    "./context": {
      "import": "./dist/context.es.js",
      "require": "./dist/context.cjs.js",
      "types": "./dist/context/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.es.js",
      "require": "./dist/hooks.cjs.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.es.js",
      "require": "./dist/utils.cjs.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./config": {
      "import": "./dist/config.es.js",
      "require": "./dist/config.cjs.js",
      "types": "./dist/config/index.d.ts"
    },
    "./preset": {
      "import": "./dist/preset.es.js",
      "require": "./dist/preset.cjs.js",
      "types": "./dist/preset.d.ts"
    },
    "./icons": {
      "import": "./dist/icons.es.js",
      "require": "./dist/icons.cjs.js",
      "types": "./dist/icons.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "context": [
        "./dist/context/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "config": [
        "./dist/config/index.d.ts"
      ],
      "preset": [
        "./dist/preset.d.ts"
      ],
      "icons": [
        "./dist/icons.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublish": "tsc && vite build"
  },
