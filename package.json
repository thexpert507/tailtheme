{
  "name": "tailtheme",
  "private": false,
  "version": "1.0.11",
  "type": "module",
  "license": "Apache-2.0",
  "access": "public",
  "author": {
    "name": "Adriel Avila",
    "email": "adrielarnel00@gmail.com"
  },
  "exports": {
    "./components": {
      "import": "./dist/components.es.js",
      "require": "./dist/components.cjs.js"
    },
    "./context": {
      "import": "./dist/context.es.js",
      "require": "./dist/context.cjs.js"
    },
    "./hooks": {
      "import": "./dist/hooks.es.js",
      "require": "./dist/hooks.cjs.js"
    },
    "./utils": {
      "import": "./dist/utils.es.js",
      "require": "./dist/utils.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "context": [
        "./dist/context/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublish": "tsc && vite build"
  },
  "dependencies": {
    "@legendapp/state": "^1.11.3",
    "@react-three/drei": "^9.87.1",
    "@react-three/fiber": "^8.14.5",
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropdown-select": "^4.10.0",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0"
  }
}